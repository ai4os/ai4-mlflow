    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: mlflow",
            "type": "debugpy",
            "request": "launch",
            "module": "mlflow",
            "args": [
                "server",
                "--app-name=basic-auth",
                "--workers=1",
                "--dev"
            ],
            "env": {
                "MLFLOW_AUTH_CONFIG_PATH": "${workspaceFolder}/backend/srv/auth_development.ini",

            },
            "cwd": "${workspaceFolder}/backend/srv"
        }
    ]
}